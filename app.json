{
  "expo": {
    "name": "Pacemaker",
    "slug": "pacemaker",
    "scheme": "pacemaker",
    "privacy": "unlisted",
    "platforms": ["ios", "android", "web"],
    "version": "1.4.0",
    "orientation": "portrait",
    "icon": "./src/assets/icon.png",
    "splash": {
      "image": "./src/assets/splash.png",
      "resizeMode": "cover"
    },
    "updates": {
      "fallbackToCacheTimeout": 5,
      "checkAutomatically": "ON_ERROR_RECOVERY"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "buildNumber": "1",
      "supportsTablet": false,
      "bundleIdentifier": "com.madebybread.pacemakerid"
    },
    "android": {
      "package": "com.madebybread.pacemakerid",
      "versionCode": 17
    },
    "description": "Pacemaker ID ",
    "packagerOpts": {
      "sourceExts": ["js", "json", "ts", "tsx", "jsx"]
    }
  }
}
